# runna-intervals environment variables
# Copy to .env and fill in your values, OR run `runna-intervals config` to set up automatically.
#
# Find your Intervals.icu API key:
#   https://intervals.icu → Settings → Developer Settings → Generate API Key
#
# Find your athlete ID in your profile URL:
#   https://intervals.icu/i12345  →  athlete ID is "i12345"

RUNNA_INTERVALS_INTERVALS_API_KEY=your_api_key_here
RUNNA_INTERVALS_INTERVALS_ATHLETE_ID=i12345

# Runna private ICS calendar feed URL (from the Runna app → Profile → Connected Apps & Devices → Connect Calendar → Other Calendar)
RUNNA_INTERVALS_RUNNA_ICS_URL=https://cal.runna.com/your-private-calendar.ics

#The easy-pace fallback is used for steps Runna describes as "conversational pace" without giving a specific pace.
RUNNA_INTERVALS_EASY_PACE_SEC_MI=480   # 8:00/mi
